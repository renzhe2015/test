<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_getallapp"
    android:layout_width="300.0px"
    android:layout_height="41.333336px"
    android:orientation="horizontal"
    android:paddingLeft="0.0px"
    android:paddingRight="6.666667px"
    android:layout_gravity="center_vertical">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|center_vertical" 
        android:addStatesFromChildren="true"
        android:orientation="horizontal">

        <TextView
	        android:id="@+id/textview"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="22.0px"
	        android:textColor="@color/item_menu_text_color"
	        android:singleLine="true"
	        android:layout_gravity="center"
	        android:layout_alignParentLeft="true"
	        android:ellipsize="marquee"
	        android:paddingLeft="3.3333335px"
	        android:layout_marginTop="3.3333335px"/>
        

        <ImageView
            android:id="@+id/imageview"
            android:contentDescription="@string/string_null"
            android:layout_marginTop="10.0px"
            android:layout_width="6.0px"
            android:layout_height="11.333334px"
            android:background="@drawable/arrow_right_unfocus"
            android:gravity="center"
	        android:layout_alignParentRight="true"
	        android:layout_marginLeft="15.333334px"/>
        
        <com.tsb.settings.settings.view.AutoRunTextView
            android:id="@+id/textvalue"
        	android:layout_width="120.0px"
	        android:layout_height="wrap_content"
	        android:textSize="22.0px"
	        android:textColor="@color/item_menu_text_color"
	        android:focusable="true"
	        android:singleLine="true"
	        android:layout_gravity="center"
	        android:ellipsize="marquee"
            android:gravity="right" 
            android:layout_toLeftOf="@id/imageview"
	        android:layout_marginTop="3.3333335px"/>
    </RelativeLayout>
    <ImageView
        android:id="@+id/divider_below"
        android:contentDescription="@string/string_null"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:gravity="center_horizontal"/>
</FrameLayout>
