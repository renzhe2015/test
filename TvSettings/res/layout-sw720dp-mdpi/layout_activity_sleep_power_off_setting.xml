<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/poweroff_settings"
    android:layout_width="1280.0px"
    android:layout_height="720.0px"
    android:background="@color/transparent"      
    android:focusable="true">

    <TextView
        android:id="@+id/sleep_off_close"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"      
        android:layout_marginLeft="166.66667px"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:background="@drawable/sleep_poweroff_bg_left"
        android:focusable="true"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_close"
        android:textColor="@color/white_color"
        android:textSize="22.5px" >
        <requestFocus>        
	    </requestFocus>  
    </TextView>

    <ImageView
        android:id="@+id/sleep_poweroff_focus"
        android:layout_width="125.333336px"
        android:layout_height="88.0px"
        android:layout_marginLeft="162.66667px"
        android:layout_marginTop="564.0px"
        android:background="@drawable/sleep_poweroff_bg_focus"
        android:contentDescription="@string/app_name" />

    <TextView
        android:id="@+id/sleep_off_10"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_close"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:focusable="true"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_10min"
        android:textColor="@color/white_color"
        android:textSize="22.5px" />

    <TextView
        android:id="@+id/sleep_off_30"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_10"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:focusable="true"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_30min"
        android:textColor="@color/white_color"
        android:textSize="22.5px" />

    <TextView
        android:id="@+id/sleep_off_60"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_30"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:focusable="true"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_60min"
        android:textColor="@color/white_color"
        android:textSize="22.5px" />
    
    <TextView
        android:id="@+id/sleep_off_90"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_60"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_90min"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:textColor="@color/white_color"
        android:focusable="true" 
        android:textSize="22.5px">
    </TextView>

    <TextView
        android:id="@+id/sleep_off_120"
    	android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_90"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:focusable="true"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_120min"
        android:textColor="@color/white_color"
        android:textSize="22.5px" />
    
     <TextView
        android:id="@+id/sleep_off_180"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_120"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_180min"
        android:background="@drawable/sleep_poweroff_bg_mid"
        android:textColor="@color/white_color"
        android:focusable="true" 
        android:textSize="22.5px">
    </TextView>
    
    <TextView
        android:id="@+id/sleep_off_240"
        android:layout_width="@dimen/tcl_time_power_off_text_width"
        android:layout_height="@dimen/tcl_time_power_off_text_height"
        android:layout_toRightOf="@id/sleep_off_180"
        android:layout_marginLeft="@dimen/tcl_time_power_off_left_margin"
        android:layout_marginTop="@dimen/tcl_time_power_off_top_margin"
        android:gravity="center"
        android:text="@string/tcl_sleepoff_240min"
        android:background="@drawable/sleep_poweroff_bg_right"
        android:textColor="@color/white_color"
        android:focusable="true" 
        android:textSize="22.5px">
    </TextView>
      
</RelativeLayout>
